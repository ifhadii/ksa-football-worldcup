RewriteEngine On    # Turn on the rewriting engine

RewriteRule    ^dashboard/?$    index.php    [NC,L]
RewriteRule    ^login/?$    login.php    [NC,L]
RewriteRule    ^register/([A-Za-z0-9-]+)/?$    register.php?ref=$1    [NC,L]
RewriteRule    ^logout/?$    logout.php    [NC,L]

RewriteRule    ^profile/?$    profile.php    [NC,L]

RewriteRule    ^createevent/?$    add-event.php    [NC,L]
RewriteRule    ^createcity/?$    add-city.php    [NC,L]
RewriteRule    ^createsocial/?$    add-social.php    [NC,L]
RewriteRule    ^addwhy/?$    add-why.php    [NC,L]

RewriteRule    ^editevent/([A-Za-z0-9-]+)/?$    editport.php?id=$1    [NC,L]
RewriteRule    ^editcity/([A-Za-z0-9-]+)/?$    editcity.php?id=$1    [NC,L]

RewriteRule    ^event/?$    event.php    [NC,L]

RewriteRule    ^sections/?$    section-title.php    [NC,L]
RewriteRule    ^city/?$    city.php    [NC,L]

RewriteRule    ^settings/?$    site-settings.php    [NC,L]
RewriteRule    ^social/?$    social-links.php    [NC,L]

RewriteRule    ^why/?$    why-us.php    [NC,L]
RewriteRule    ^logo/?$    updatelogo.php    [NC,L]

RewriteRule    ^createblog/?$    add-blog.php    [NC,L]
RewriteRule    ^blog/?$    bloglist.php    [NC,L]

RewriteRule    ^createslide/?$    add-slider.php    [NC,L]
RewriteRule    ^slider/?$    sliderlist.php    [NC,L]
RewriteRule    ^static/?$    static-home.php    [NC,L]
RewriteRule    ^contact/?$    updatecontact.php    [NC,L]

RewriteRule    ^testimony/?$    testimonylist.php    [NC,L]
RewriteRule    ^newtestimony/?$    add-testimony.php    [NC,L]
